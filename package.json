{
  "name": "foss4g2019",
  "version": "1.0.0",
  "description": "FOSS4G 2019 presentation with Eagle.js",
  "author": "sookoll",
  "repository": {
    "type": "git",
    "url": "https://github.com/sookoll/foss4g2019.git"
  },
  "license": "BSD",
  "scripts": {
    "dev": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy": "NODE_ENV=production npm run build && chmod +x ./scripts/deploy.sh && ./scripts/deploy.sh"
  },
  "dependencies": {
    "animate.css": "^3.7.2",
    "chart.js": "2.5.0",
    "eagle.js": "^0.4.4",
    "highlight.js": "^9.15.10",
    "vue": "^2.6.10",
    "vue-router": "^2.2.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/eslint-config-standard": "^3.0.5",
    "node-sass": "^4.12.0",
    "pug": "^2.0.4",
    "pug-plain-loader": "^1.0.0",
    "sass-loader": "^7.3.1",
    "vue-template-compiler": "^2.6.10"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {
      "arrow-parens": 0,
      "generator-star-spacing": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
